<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cc.xpress.mitaole.user.dto.ClassifyTbDTO" table="classify_tb" schema="mitaole_db">
        <id name="classifyId">
            <column name="classify_id" sql-type="int(11)"/>
        </id>
        <property name="classifyName">
            <column name="classify_name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>