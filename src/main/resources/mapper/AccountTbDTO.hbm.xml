<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cc.xpress.mitaole.user.dto">
    <class name="AccountTbDTO" table="account_tb" schema="mitaole_db" >
        <id name="accountId">
            <column name="account_id" sql-type="bigint(20)"/>
            <generator class="native"/>
        </id>
        <property name="accountTotal">
            <column name="account_total" sql-type="int(11)"/>
        </property>
        <property name="accountFreeze">
            <column name="account_freeze" sql-type="int(11)"/>
        </property>
        <property name="accountBalance">
            <column name="account_balance" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>